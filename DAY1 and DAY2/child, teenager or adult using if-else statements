#include <iostream>

using namespace std;

int main()
{   int age;
    
    cout<<"\n Enter age of the person:";
    cin>>age;
    
    if((age>=0)&&(age<=12))
     {
        cout<<"\n The person is a child";
        
     }
    else if((age>=13)&&(age<=19))
     {
        cout<<"\n The person is a teenager";
     }
    else
      cout<<"\n The person is an adult";
    
    return 0;
    
}

