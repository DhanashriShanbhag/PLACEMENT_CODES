#include <iostream>

using namespace std;

int main()
{   int a,b;
    char op;
    cout<<"\n Enter two numbers:";
    cin>>a>>b;
    
    cout<<"\n Enter an operation(+,-,*,/):";
    cin>>op;
    
    switch(op)
     {
        case '+':
         
            cout<<"Sum="<<a+b;
            break;
            
        case '-':
           
            cout<<"Difference="<<a-b;
            break;
        
        case '*':
          
            cout<<"Product="<<a*b;
            break;
            
        case '/':
        
            cout<<"Quotient="<<a/b;
            break;
            
        default: 
          
            cout<<"Error!";
            break;
     }
    
    
    return 0;
    
}
